{
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/login",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/Authentication/Login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8081
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/logout",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/Authentication/Logout",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8081
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/patients",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/Patient/GetAll",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8081
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/patient/get?id={id}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/Patient/Get?id={id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8081
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/patient/create",
      "UpstreamHttpMethod": [ "Post" ],
