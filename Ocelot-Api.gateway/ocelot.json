      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8081
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/patient/delete?id={id}",
      "UpstreamHttpMethod": [ "Delete" ],
      "DownstreamPathTemplate": "/api/Patient/Delete?id={id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8081
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "UpstreamPathTemplate": "/gateway/patient/update?id={id}",
      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamPathTemplate": "/api/Patient/Update?id={id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8081
        }
      ],
      "DangerousAcceptAnyServerCertificateValidator": true
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000",
    "RateLimitOptions": {
      "DisableRateLimitHeaders": true
    }
  }
}