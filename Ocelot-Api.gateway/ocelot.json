      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamPathTemplate": "/api/Patient/Update?id={id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "gestion-patients.api",
          "Port": 8080
        }
      ]
